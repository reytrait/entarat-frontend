{
    "framework": "nextjs",
    "buildCommand": "npm run build:web",
    "outputDirectory": ".next",
    "$schema": "https://vercel.com/schemas/v29.json",
    "projectSettings": {},
    "rootDirectory": "apps/web",
    "builds": [
      {
        "src": "apps/web/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  